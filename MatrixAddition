public class MatrixAdd {
    public static void main(String args[]) {
      int a[][]={{1,0,5},{1,2,3},{1,5,6}};
      int b[][]={{8,0,4},{3,2,1},{3,5,7}};
      int c[][]=new int[3][3];
    
        for(int i=0; i<3; i++) {
            for(int j=0; j<3; j++) {
                c[i][j] = a[i][j] + b[i][j];
                System.out.print(c[i][j]+" "); 
            }
            System.out.println();
        }
    }
}
