public class Main { 
    public void gcd(int x, int y) {
        int gcd = 1;  
        for(int i = 1; i <= x && i <= y; i++) {  
            if(x%i==0 && y%i==0) { 
                gcd = i;  
            }  
        }
        System.out.printf("GCD of %d and %d is: %d", x, y, gcd);  
    }
    public static void main(String[] args) {  
        Main main = new Main();
        main.gcd(12, 8);
    }  
}  
